@import './style/variable'
@import './style/mixin'
scroll-bar()
#app
  display flex
  flex-direction column
  height 100%
  &>.nav
    line-height 40px
    background #222
    flex 0 0 40px
    .link
      display inline-block
      margin-right 20px
      color #999
      &:hover, &.router-link-active
        color #fff
  &>.content
    flex 1
    min-height 0
